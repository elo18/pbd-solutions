public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("EEEEE  L");
		System.out.println("E      L");
		System.out.println("E      L");
		System.out.println("EEE    L");
		System.out.println("E      L");
		System.out.println("E      L");
		System.out.println("EEEEE  LLLLL");
	}
}
